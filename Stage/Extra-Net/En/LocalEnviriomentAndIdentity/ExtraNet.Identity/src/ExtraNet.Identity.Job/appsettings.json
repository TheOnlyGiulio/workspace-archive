{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "KeycloakClient": {
    "RealmName": "extra-net",
    "KeycloakBaseUrl": null,
    "UriScheme": "/admin/realms/",
    "ClientId": "admin-cli",
    "Username": null,
    "Password": null,
    "KeycloakCreateRealm": "/",
    "KeycloakImportRealm": "/{realm}/partialImport",
    "KeycloakCreateClient": "/{realm}/clients",
    "KeycloakGetClient": "/{realm}/clients",
    "KeycloakUpdateClient": "/{realm}/clients/{client-uuid}",
    "KeycloakDeleteClient": "/{realm}/clients/{client-uuid}",
    "KeycloakCreateGroup": "/{realm}/groups",
    "KeycloakGetGroup": "/{realm}/groups",
    "KeycloakUpdateGroup": "/{realm}/groups/{group-id}",
    "KeycloakDeleteGroup": "/{realm}/groups/{group-id}",
    "KeycloakCreateRealmRole": "/{realm}/roles",
    "KeycloakGetRealmRole": "/{realm}/roles",
    "KeycloakUpdateRealmRole": "/{realm}/roles-by-id/{role-id}",
    "KeycloakDeleteRealmRole": "/{realm}/roles-by-id/{role-id}",
    "KeycloakCreateClientRole": "/{realm}/clients/{client-uuid}/roles",
    "KeycloakGetClientRole": "/{realm}/clients/{client-uuid}/roles",
    "KeycloakUpdateClientRole": "/{realm}/clients/{client-uuid}/roles/{role-name}",
    "KeycloakDeleteClientRole": "/{realm}/clients/{client-uuid}/roles/{role-name}",
    "KeycloakCreateClientScope": "/{realm}/client-scopes",
    "KeycloakGetClientScope": "/{realm}/client-scopes",
    "KeycloakUpdateClientScope": "/{realm}/client-scopes/{client-scope-id}",
    "KeycloakDeleteClientScope": "/{realm}/client-scopes/{client-scope-id}",
    "KeycloakCreateProtocolMapper": "/{realm}/client-scopes/{client-scope-id}/protocol-mappers/models",
    "KeycloakGetProtocolMapper": "/{realm}/client-scopes/{client-scope-id}/protocol-mappers/models",
    "KeycloakUpdateProtocolMapper": "/{realm}/client-scopes/{client-scope-id}/protocol-mappers/models/{id}",
    "KeycloakDeleteProtocolMapper": "/{realm}/client-scopes/{client-scope-id}/protocol-mappers/models/{id}",
    "KeycloakCreateRealmSettings": "/{realm}",
    "KeycloakGetRealmSettings": "/{realm}",
    "KeycloakPutRealmSettings": "/{realm}",
    "KeycloakDeleteRealmSettings": "/{realm}",
    "KeycloakToken": "/realms/master/protocol/openid-connect/token",
    "KeycloakAssignRealmRoleToGroup": "/{realm}/groups/{group-id}/role-mappings/realm",
    "KeycloakUnassignRealmRoleToGroup": "/{realm}/groups/{group-id}/role-mappings/realm",
    "KeycloakGetRealmRolesOfGroup": "/{realm}/groups/{group-id}/role-mappings/realm"
  }
}